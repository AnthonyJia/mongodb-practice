{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "program":"${file}",
            "request": "launch",
            "console": "integratedTerminal",
            "env": {
                "MONGOPASS": "${env:MONGOPASS}"
            },
        }
    ]
}